<div *ngIf="!partnerIsRegistrationCompleted" >
<div class="userprofilewidget">
    <div class="username">
        <h3>
            <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
