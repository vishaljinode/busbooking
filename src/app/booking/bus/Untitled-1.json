<!-- Update Modal -->
<ng-template #update let-modal>
    <div class="deletemodal">
        <button type="button" class="btn-close" aria-label="Close" (click)="modal.dismiss('Cross click')">
            <img src="../../../../../assets/images/cross.svg" />
        </button>
        <h2>Upload Photo</h2>
        <div class="fileuploadupdate">
            <label>Upload Photo</label>
            <div class="fileupload">
                <input type="file" />
                <div class="filecontent">
                    <p>Drag and Drop </p>
                    <p>or</p>
                    <button mat-raised-button class="" color="primary">Browse</button>
                </div>
            </div>
            <ul>
                <li>
                    <img src="../../../../../assets/images/marketuser.png" />
                    <span><img src="../../../../../assets/images/crossblack.svg" /></span>
                </li>
                <li>
                    <img src="../../../../../assets/images/marketuser.png" />
                    <span><img src="../../../../../assets/images/crossblack.svg" /></span>
                </li>
                <li>
                    <img src="../../../../../assets/images/marketuser.png" />
                    <span><img src="../../../../../assets/images/crossblack.svg" /></span>
                </li>
                <li>
                    <img src="../../../../../assets/images/marketuser.png" />
                    <span><img src="../../../../../assets/images/crossblack.svg" /></span>
                </li>
            </ul>
        </div>
        <div class="actionbtn">
            <button mat-raised-button class="" color="primary" (click)="modal.close('Close click')">Upload</button>
        </div>
    </div>
</ng-template>


